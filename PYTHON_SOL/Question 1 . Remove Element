class Solution(object):
    def removeElement(self, nums, val):
        n = len(nums)
        i = 0
        length = 0
        for i in range(0,n):
            if nums[i] != val:
                nums[length] = nums[i]
                length +=1
                
                
        return length
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        
